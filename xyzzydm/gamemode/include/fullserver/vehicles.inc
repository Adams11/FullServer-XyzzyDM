	Msg(playerid,COLOR_INFO,"Lista pojazdow jest tymczasowa, wkrotce zostanie wprowadzona pelna. VIPy moga skorzystac z komendy {b}/p <nazwa/id>{/b}");

	if (GetPlayerMoney(playerid)<5000)
		return Msg(playerid,COLOR_ERROR, "Nie stac Cie na zakup zadnego pojazdu.");
	new buf[255],i;
	format(buf, sizeof buf, "select vid,name,cena from fs_vehicles where minPoziom='gracz' and jezdzi=1 and cena<%d and przyczepa=0 order by rand() limit 10;", GetPlayerMoney(playerid));
	mysql_query(buf);
	mysql_store_result();
	if (mysql_num_rows()==0) {
			mysql_free_result();
			return Msg(playerid,COLOR_ERROR,"Nie znaleziono zadnego pojazdu ktory moglbys kupic");
		}
	new txt_lista[1024];
	while (mysql_fetch_row(buf,"|")) {
		new vid, vname[32],cena;
		if (i++>0) strcat(txt_lista,"\n");
		if(sscanf(buf,"p<|>ds[32]d",vid,vname,cena)) continue;
		format(txt_lista,sizeof txt_lista,"%s{000000}%d{ffffff} %s %d$", txt_lista, vid, vname, cena);
	}
	mysql_free_result();
	return ShowPlayerDialog(playerid,DIALOG_POJAZDY_LISTA,DIALOG_STYLE_LIST,"Tymczasowe menu pojazdow:",txt_lista,"OK","Anuluj");
}


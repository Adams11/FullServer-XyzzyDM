vehicles_DriftSubmission(playerid,vid){	// penisowa nazwa ale niech bedzie
	// odwracamy auto i naprawiamy
	new Float:zangle;
	GetVehicleZAngle(vid,zangle);
	SetVehicleZAngle(vid,zangle);

	PlaySound(playerid, 1054);

	new cost=2000;
	new Float:vhp;
	GetVehicleHealth(vid,vhp);
	if (vhp>10000)	// vehgod
		return 1;	// nie naprawiamy
	if (vhp>VEHICLE_DEFAULT_HP) vhp=VEHICLE_DEFAULT_HP;
	cost=2000+(VEHICLE_DEFAULT_HP-floatround(vhp));

	new money;
	money=GetPlayerMoney(playerid);
	if (money < cost)
		return Msg(playerid,COLOR_ERROR,"Nie stac Cie na naprawe auta");	// nie stac Cie na to
	money-=cost;
	SetPlayerMoney(playerid,money);
	RepairVehicle(vid);
	SetVehicleHealth(vid,VEHICLE_DEFAULT_HP);

	tVehicleUsed[vid]=true;
	tVehicles[vid][vo_used]=true;


	return 1;
}

